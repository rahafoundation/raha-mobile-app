{
  "name": "@raha.app/mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:ios": "react-native run-ios",
    "start:android:test": "yarn config:test && react-native run-android --variant=devTestDebug --appId=app.raha.mobileTest",
    "start:android:prod": "yarn config:prod && react-native run-android --variant=devProdDebug --appId=app.raha.mobileProd",
    "test": "jest",
    "config:prod": "cp ./src/data/prod.config.ts ./src/data/config.ts && cp ./ios/data/config/firebase/GoogleService-Info.prod.plist ./ios/mobile/GoogleService-Info.plist && cp ./android/data/config/firebase/google-services.prod.json ./android/app/google-services.json",
    "config:test": "cp ./src/data/test.config.ts ./src/data/config.ts && cp ./ios/data/config/firebase/GoogleService-Info.test.plist ./ios/mobile/GoogleService-Info.plist && cp ./android/data/config/firebase/google-services.test.json ./android/app/google-services.json",
    "bundle:android": "yarn config:prod && cd android && ./gradlew assembleProdRelease && cd ..",
    "codepush:android:release": "appcenter codepush release-react --app rahafoundation/Raha-App -d Staging ",
    "codepush:android:promote": "appcenter codepush promote --app rahafoundation/Raha-App -s Staging -d Production",
    "codepush:android:history:staging": "appcenter codepush deployment history Staging --app rahafoundation/Raha-App",
    "codepush:android:history:production": "appcenter codepush deployment history Production --app rahafoundation/Raha-App",
    "codepush:ios:release": "appcenter codepush release-react --app rahafoundation/iOS-Raha-App -d Staging --plist-file ios/mobile/Info.plist",
    "codepush:ios:promote": "appcenter codepush promote --app rahafoundation/iOS-Raha-App -s Staging -d Production",
    "codepush:ios:history:staging": "appcenter codepush deployment history Staging --app rahafoundation/iOS-Raha-App",
    "codepush:ios:history:production": "appcenter codepush deployment history Production --app rahafoundation/iOS-Raha-App"
  },
  "dependencies": {
    "@raha/api": "^0.2.5",
    "@raha/api-shared": "^0.1.6",
    "appcenter": "^1.7.1",
    "big.js": "^5.1.2",
    "core-js": "^2.5.7",
    "date-fns": "^2.0.0-alpha.16",
    "es6-symbol": "^3.1.1",
    "google-libphonenumber": "^3.1.8",
    "immutable": "^4.0.0-rc.9",
    "react": "16.4.1",
    "react-native": "^0.56.0",
    "react-native-camera": "^1.1.4",
    "react-native-code-push": "^5.4.1",
    "react-native-country-picker-modal": "^0.6.2",
    "react-native-device-info": "^0.21.5",
    "react-native-dropdownalert": "^3.4.0",
    "react-native-elements": "^0.19.1",
    "react-native-firebase": "^5.0.0-rc1",
    "react-native-svg": "^6.5.2",
    "react-native-vector-icons": "^5.0.0",
    "react-native-video": "^2.3.1",
    "react-native-video-processing": "shahen94/react-native-video-processing",
    "react-navigation": "^2.0.1",
    "react-navigation-tabs": "^0.6.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.9.1",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "url": "^0.11.0",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "@types/big.js": "^4.0.4",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/google-libphonenumber": "^7.4.16",
    "@types/jest": "^23.1.5",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.12",
    "@types/react-native-vector-icons": "^4.6.3",
    "@types/react-native-video": "^2.0.4",
    "@types/react-navigation": "^2.0.11",
    "@types/react-redux": "^6.0.0",
    "@types/react-test-renderer": "^16.0.1",
    "@types/validator": "^9.4.1",
    "appcenter-cli": "^1.1.1",
    "babel-jest": "^23.4.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react-native": "^5.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.4.0",
    "jsdom": "^11.10.0",
    "react-native-mock-render": "^0.0.25",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.3.2",
    "redux-devtools-extension": "^2.13.5",
    "ts-jest": "^23.1.3",
    "tslib": "^1.9.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "jest": {
    "preset": "react-native",
    "globals": {
      "ts-jest": {
        "useBabelrc": true,
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "testRegex": "/src/.*\\.test\\.(ts|tsx)?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-navigation-tabs|react-native-vector-icons|react-native-paper|react-navigation-tabs|react-native-elements|react-native-video|react-navigation|react-native-country-picker-modal|react-native-safe-area-view|react-native-device-info|react-native-dropdownalert|react-native-camera)/)"
    ],
    "setupFiles": [
      "<rootDir>/scripts/jest/setup",
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js"
    ]
  }
}
