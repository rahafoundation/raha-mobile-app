{
  "name": "@raha.app/mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "link-assets": "react-native link",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:ios": "react-native run-ios",
    "start:android:test": "yarn config:test && react-native run-android --variant=devTestDebug --appId=app.raha.mobileTest",
    "start:android:prod": "yarn config:prod && react-native run-android --variant=devProdDebug",
    "test": "jest",
    "config:prod": "cp ./src/data/prod.config.ts ./src/data/config.ts && cp ./ios/data/config/firebase/GoogleService-Info.prod.plist ./ios/mobile/GoogleService-Info.plist && cp ./android/data/config/firebase/google-services.prod.json ./android/app/google-services.json",
    "config:test": "cp ./src/data/test.config.ts ./src/data/config.ts && cp ./ios/data/config/firebase/GoogleService-Info.test.plist ./ios/mobile/GoogleService-Info.plist && cp ./android/data/config/firebase/google-services.test.json ./android/app/google-services.json"
  },
  "dependencies": {
    "big.js": "^5.1.2",
    "core-js": "^2.5.7",
    "date-fns": "^1.29.0",
    "es6-symbol": "^3.1.1",
    "firebase": "^5.1.0",
    "immutable": "^4.0.0-rc.9",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-camera": "^1.1.4",
    "react-native-dropdownalert": "^3.4.0",
    "react-native-elements": "^0.19.1",
    "react-native-firebase": "^4.2.0",
    "react-native-google-signin": "^0.12.0",
    "react-native-vector-icons": "^4.6.0",
    "react-native-video": "^2.3.1",
    "react-navigation": "^2.0.1",
    "react-navigation-material-bottom-tabs": "^0.3.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.9.1",
    "redux-persist-transform-immutable": "^5.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@types/big.js": "^4.0.4",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.3.14",
    "@types/react-native": "^0.55.12",
    "@types/react-native-google-signin": "^0.12.2",
    "@types/react-native-vector-icons": "^4.6.1",
    "@types/react-native-video": "^2.0.4",
    "@types/react-navigation": "^2.0.6",
    "@types/react-redux": "^6.0.0",
    "@types/react-test-renderer": "^16.0.1",
    "babel-jest": "23.0.1",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jsdom": "^11.10.0",
    "react-native-mock-render": "^0.0.25",
    "react-native-scripts-ts": "1.15.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.3.2",
    "remote-redux-devtools": "^0.5.12",
    "ts-jest": "^22.4.6",
    "tslib": "^1.9.0",
    "typescript": "^2.8.3"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts/"
    ]
  },
  "jest": {
    "preset": "react-native",
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [
      "<rootDir>/scripts/jest/setup"
    ]
  }
}
